<div id="vcomments"></div>
<script>
function LoadValine() {
    mengd.getScript("<%-theme.valine.source%>", function () {
        new Valine(Object.assign({
            el: '#vcomments',
            path: window.location.pathname,
            appId: '<%- theme.valine.appid %>',
            appKey: '<%- theme.valine.appkey %>',
            visitor: true
        }, <%- JSON.stringify(theme.valine) %>))
    });
}
LoadValine()
</script>