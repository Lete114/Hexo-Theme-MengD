<% var options = theme.twikoo.options ? JSON.stringify(theme.twikoo.options) : false %>
<div id="tcomment"></div>
<script>
function LoadTwikoo(){
  mengd.getScript("<%-theme.twikoo.source%>",function(){
    initData = {
      el: '#tcomment',
      envId: '<%-theme.twikoo.envId%>',
      region: '<%-theme.twikoo.region%>',
      path: '<%-theme.twikoo.path%>',
  }
  var options = <%-options%>;
  if(options){
      initData = Object.assign(initData, options)
  }
  twikoo.init(initData);
  })
}
LoadTwikoo()
</script>