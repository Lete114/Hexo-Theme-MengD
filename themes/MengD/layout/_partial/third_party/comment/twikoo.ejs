<div id="tcomment"></div>
<script>
function LoadTwikoo(){
  mengd.getScript("<%-theme.twikoo.source%>",function(){
  twikoo.init(Object.assign({
      el: '#tcomment',
      envId: '<%-theme.twikoo.envId%>',
      region: '<%-theme.twikoo.region%>',
      path: location.pathname,
  }, <%-JSON.stringify(theme.twikoo)%>));
  })
}
LoadTwikoo()
</script>