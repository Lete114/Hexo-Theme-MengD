<% if (theme.search && config.search) { %>
    <div id="local-search">
        <div id="local-search-title"><%= __('search.title') %> </div>
        <input id="local-search-input" autocomplete="off" placeholder="<%= __('search.input') %>"
            type="text">
        <hr>
        <div id="local-search-result"></div>
        <span class="search-close-button"><i class="fas fa-times"></i></span>
    </div>
    <script src="<%= url_for(theme.CDN.search) %>"></script>
<% } %>