<% 

var codeBlockExpand = theme.codeBlockExpand

%>

<script>
  var $config = {
      CDN: {
        fancyboxJs: "<%=theme.CDN.fancyboxJs%>",
        fancyboxCss: "<%=theme.CDN.fancyboxCss%>"
      },
      <%if(theme.search&&config.search){%>
      searchFile: "<%=url_for(config.search.path)%>",
      <%}%>
      <%if(codeBlockExpand.enable){%>
      codeBlockExpand: {
        enable: <%=codeBlockExpand.enable%>,
        height: <%=codeBlockExpand.height%>,
        scrollTop: <%=codeBlockExpand.scrollTop%>
      }
      <%}%>
    }
</script>