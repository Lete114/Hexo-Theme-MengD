
<!DOCTYPE html>
<html>
<head>
    <%- partial('_global/head') %>
</head>

<body>
    <%- partial('_global/import_top') %> 
    <%- partial('_global/config') %>
    <%- partial('_partial/third_party/analytics') %>
    <div id="body-wrap">
        <%- partial('_global/nav') %>

        <%
        
        var propertyName= is_home()?'id':'class' 
        var propertyValue= 'main'
        var property = `${propertyName}=${propertyValue}`
        
        %> 
        <main <%= property %>>
            <%- body %>
        </main>
        <%- partial('_partial/rightside') %>
        <%- partial('_global/footer') %>
    </div>
    <div id="mask" onclick="closeAll()"></div>
    <%- partial('_global/search') %>
    <div class="script">
        <script src="<%= theme.CDN.mainJs %>"></script>
        <%- partial('_global/import_bottom') %>
        <%- partial('_partial/third_party/pjax') %>
    </div>
</body>

</html>