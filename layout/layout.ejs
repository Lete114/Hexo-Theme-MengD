<%# 全局页面都会需要用到这个模板 %>
<%# partial()方法可以导入其他ejs文件 %>  
<!DOCTYPE html>
<html>
<head>
    <%- partial('_global/head') %>
</head>

<body>
    <%-partial('_global/import_top')%> 
    <%- partial('_global/config') %>
    <%- partial('_partial/third_party/analytics') %>
    <div id="body_wrap">
        <%- partial('_global/nav') %>
        <div id="main_wrap">
                <main class="main">
                    <%# 写的文章内容都将会显示到这里 %> 
                    <%- body %>
                </main>
        </div>
        <%- partial('_partial/rightside') %>
        <%- partial('_global/footer') %>
    </div>
    <div id="mask" onclick="close_all()"></div>
    <%- partial('_global/search') %>
    <div class="script">
        <script src="<%-theme.CDN.main_js%>"></script>
        <%- partial('_global/import_bottom')%>
        <%- partial('_partial/third_party/pjax') %>
    </div>
</body>

</html>